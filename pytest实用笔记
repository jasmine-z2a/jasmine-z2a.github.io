        logger.info(f&quot;æµ‹è¯•ç”¨ä¾‹:&#123;item.name&#125;&quot;)
        logger.info(f&quot;å¼‚å¸¸ä¿¡æ¯:&#123;call.excinfo&#125;&quot;)
